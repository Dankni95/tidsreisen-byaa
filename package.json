{
  "name": "Tidsreisen Byaaa",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "cd server && node server.js",
    "build": "npm ci --include=dev && npm install && npm run build:parcel",
    "build:parcel": "cd client && parcel build index.html",
    "clean": "rimraf client/dist dist .parcel-cache ",
    "dev": "concurrently npm:dev:server npm:dev:client",
    "dev:client": "cd client && parcel watch index.html",
    "dev:server": "cd server && nodemon server.js"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "bootstrap": "^5.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mapbox-gl": "^2.8.2",
    "mapbox-gl-markers": "^0.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.3.1",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.3",
    "workbox-broadcast-update": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-google-analytics": "^6.5.3",
    "workbox-navigation-preload": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-streams": "^6.5.3",
    "parcel": "^2.3.2"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  }
}
